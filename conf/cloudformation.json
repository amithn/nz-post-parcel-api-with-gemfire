                    "echo 'DYNAMIC_SCRIPTS=\"Yes\"' >> /opt/pivotal/gemfire/variables.sh\n",
                    "echo 'DYNAMIC_SCRIPTS=\"No\"' >> /opt/pivotal/gemfire/variables.sh\n"
